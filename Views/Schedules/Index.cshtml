@model IEnumerable<brendanLowe_MIS4200.Models.Schedule>

@{
    ViewBag.Title = "Schedule List";
}

<h2>Schedule List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.numClasses)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tuitionCost)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.numClasses)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tuitionCost)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.scheduleID }) |
            @Html.ActionLink("Details", "Details", new { id=item.scheduleID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.scheduleID })
        </td>
    </tr>
}

</table>
